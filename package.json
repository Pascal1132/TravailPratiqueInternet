        "bootstrap-sass": "^3.3.7",
        "cross-env": "^5.1",
        "jquery": "^3.5.1",
        "jquery-ui": "^1.12.1",
        "laravel-mix": "^1.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0"
    },
    "dependencies": {
        "bootstrap": "^4.5.2",
